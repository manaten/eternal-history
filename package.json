{
  "name": "eternal-history",
  "author": "manaten@manaten.net",
  "private": true,
  "license": "MIT",
  "type": "module",
  "version": "1.0.5",
  "description": "A Chrome extension make history unlimited and searchable.",
  "scripts": {
    "dev": "vite build --watch",
    "build": "tsc -b && vite build --emptyOutDir",
    "preview": "vite preview",
    "typecheck": "tsc -b --noEmit",
    "prepare": "husky",
    "lint": "run-p lint:*",
    "lint:js": "eslint **/*.{js,jsx,mjs,ts,tsx,vue}",
    "lint-staged": "lint-staged",
    "fix": "run-s fix:*",
    "fix:js": "run-s \"lint:js -- --fix\"",
    "fix:prettier": "prettier --write --ignore-unknown .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest run",
    "gen:icons": "npx icon-gen -i ./public/assets/icon.svg -o ./public/assets -r --ico --ico-name=favicon --ico-sizes=48 --favicon --favicon-name=icon --favicon-png-sizes=16,32,48,128",
    "version": "node scripts/update-manifest-version.ts && git add public/manifest.json"
  },
  "dependencies": {
    "classnames": "^2.5.1",
    "p-map": "^7.0.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.7",
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.22.0",
    "@storybook/addon-docs": "^9.0.9",
    "@storybook/addon-onboarding": "^9.0.9",
    "@storybook/react-vite": "^9.0.9",
    "@types/chrome": "^0.0.312",
    "@types/node": "^22.13.10",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/sql.js": "^1.4.9",
    "@typescript-eslint/parser": "^8.26.1",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-functional": "^9.0.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-storybook": "^9.0.9",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.4.2",
    "react-docgen-typescript": "^2.4.0",
    "storybook": "^9.0.9",
    "tsx": "^4.19.3",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.31.0",
    "vite": "^6.2.0",
    "vitest": "^3.2.3"
  }
}
