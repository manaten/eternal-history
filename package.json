{
  "name": "eternal-history",
  "author": "manaten@manaten.net",
  "private": true,
  "license": "MIT",
  "type": "module",
  "version": "1.0.16",
  "description": "A Chrome extension make history unlimited and searchable.",
  "scripts": {
    "build-storybook": "storybook build",
    "build:dev": "npm run build && node scripts/update-manifest-name-for-dev.ts",
    "build": "tsc -b && vite build",
    "dev": "vite build --watch",
    "fix:js": "run-s \"lint:js -- --fix\"",
    "fix:prettier": "prettier --write --ignore-unknown .",
    "fix": "run-s fix:*",
    "gen:icons": "npx icon-gen -i ./public/assets/icon.svg -o ./public/assets -r --ico --ico-name=favicon --ico-sizes=48 --favicon --favicon-name=icon --favicon-png-sizes=16,32,48,128",
    "lint-staged": "lint-staged",
    "lint:js": "eslint **/*.{js,jsx,mjs,ts,tsx}",
    "lint": "run-p lint:*",
    "prepare": "husky",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "test": "vitest run",
    "typecheck": "tsc -b --noEmit",
    "version": "node scripts/update-manifest-version.ts && git add public/manifest.json"
  },
  "dependencies": {
    "@material-design-icons/svg": "^0.14.15",
    "classnames": "^2.5.1",
    "p-map": "^7.0.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.71.1"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.2",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@storybook/addon-docs": "^10.2.4",
    "@storybook/addon-onboarding": "^10.2.4",
    "@storybook/addon-vitest": "^10.2.4",
    "@storybook/react-vite": "^10.2.4",
    "@tailwindcss/vite": "^4.1.18",
    "@types/chrome": "^0.1.36",
    "@types/node": "^25.2.0",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.1.7",
    "@types/sql.js": "^1.4.9",
    "@typescript-eslint/parser": "^8.54.0",
    "@vitejs/plugin-react": "^5.1.3",
    "@vitest/browser": "^4.0.18",
    "@vitest/browser-playwright": "^4.0.18",
    "@vitest/coverage-v8": "^4.0.18",
    "chromatic": "^13.3.5",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-better-tailwindcss": "^4.1.1",
    "eslint-plugin-functional": "^9.0.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.0",
    "eslint-plugin-storybook": "^10.2.4",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.58.1",
    "prettier": "^3.8.1",
    "react-docgen-typescript": "^2.4.0",
    "storybook": "^10.1.5",
    "tailwindcss": "^4.1.11",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.54.0",
    "vite": "^7.3.1",
    "vite-plugin-svgr": "^4.5.0",
    "vitest": "^4.0.15"
  }
}
