{
  "name": "eternal-history",
  "author": "manaten@manaten.net",
  "private": true,
  "license": "MIT",
  "type": "module",
  "version": "1.0.15",
  "description": "A Chrome extension make history unlimited and searchable.",
  "scripts": {
    "dev": "vite build --watch",
    "build": "tsc -b && vite build",
    "build:dev": "npm run build && node scripts/update-manifest-name-for-dev.ts",
    "preview": "vite preview",
    "test": "vitest run",
    "typecheck": "tsc -b --noEmit",
    "prepare": "husky",
    "lint": "run-p lint:*",
    "lint:js": "eslint **/*.{js,mjs,ts,tsx}",
    "lint-staged": "lint-staged",
    "fix": "run-s fix:*",
    "fix:js": "run-s \"lint:js -- --fix\"",
    "fix:prettier": "prettier --write --ignore-unknown .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "gen:icons": "npx icon-gen -i ./public/assets/icon.svg -o ./public/assets -r --ico --ico-name=favicon --ico-sizes=48 --favicon --favicon-name=icon --favicon-png-sizes=16,32,48,128",
    "version": "node scripts/update-manifest-version.ts && git add public/manifest.json"
  },
  "dependencies": {
    "classnames": "^2.5.1",
    "p-map": "^7.0.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.31.0",
    "@storybook/addon-docs": "^9.0.9",
    "@storybook/addon-onboarding": "^9.0.9",
    "@storybook/addon-vitest": "^9.0.16",
    "@storybook/react-vite": "^9.0.9",
    "@tailwindcss/vite": "^4.1.11",
    "@types/chrome": "^0.1.1",
    "@types/node": "^24.0.13",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/sql.js": "^1.4.9",
    "@typescript-eslint/parser": "^8.36.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-better-tailwindcss": "^3.5.2",
    "eslint-plugin-functional": "^9.0.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-storybook": "^9.0.16",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "react-docgen-typescript": "^2.4.0",
    "storybook": "^9.0.9",
    "tailwindcss": "^4.1.11",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.36.0",
    "vite": "^7.0.4",
    "vitest": "^3.2.4",
    "@vitest/browser": "^3.2.4",
    "playwright": "^1.53.2",
    "@vitest/coverage-v8": "^3.2.4"
  }
}
