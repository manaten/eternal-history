{
  "name": "eternal-history",
  "author": "manaten@manaten.net",
  "private": true,
  "license": "MIT",
  "type": "module",
  "version": "1.0.15",
  "description": "A Chrome extension make history unlimited and searchable.",
  "scripts": {
    "build-storybook": "storybook build",
    "build:dev": "npm run build && node scripts/update-manifest-name-for-dev.ts",
    "build": "tsc -b && vite build",
    "dev": "vite build --watch",
    "fix:js": "run-s \"lint:js -- --fix\"",
    "fix:prettier": "prettier --write --ignore-unknown .",
    "fix": "run-s fix:*",
    "gen:icons": "npx icon-gen -i ./public/assets/icon.svg -o ./public/assets -r --ico --ico-name=favicon --ico-sizes=48 --favicon --favicon-name=icon --favicon-png-sizes=16,32,48,128",
    "lint-staged": "lint-staged",
    "lint:js": "eslint **/*.{js,jsx,mjs,ts,tsx}",
    "lint": "run-p lint:*",
    "prepare": "husky",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "test": "vitest run",
    "typecheck": "tsc -b --noEmit",
    "version": "node scripts/update-manifest-version.ts && git add public/manifest.json"
  },
  "dependencies": {
    "classnames": "^2.5.1",
    "p-map": "^7.0.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.0",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.1",
    "@storybook/addon-docs": "^10.1.5",
    "@storybook/addon-onboarding": "^10.1.5",
    "@storybook/addon-vitest": "^10.1.5",
    "@storybook/react-vite": "^10.1.5",
    "@tailwindcss/vite": "^4.1.11",
    "@types/chrome": "^0.1.3",
    "@types/node": "^24.10.2",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@types/sql.js": "^1.4.9",
    "@typescript-eslint/parser": "^8.49.0",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/browser": "^4.0.15",
    "@vitest/browser-playwright": "^4.0.15",
    "@vitest/coverage-v8": "^4.0.15",
    "chromatic": "^13.3.3",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-better-tailwindcss": "^3.8.0",
    "eslint-plugin-functional": "^9.0.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-storybook": "^10.1.5",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.54.2",
    "prettier": "^3.7.4",
    "react-docgen-typescript": "^2.4.0",
    "storybook": "^10.1.5",
    "tailwindcss": "^4.1.11",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.49.0",
    "vite": "^7.1.1",
    "vitest": "^4.0.15"
  }
}
