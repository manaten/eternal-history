{
  "manifest_version": 3,
  "name": "Eternal History on bookmarks",
  "description": "Eternal History is a Chrome extension that stores unlimited browser history data in the bookmarks area.",
  "version": "1.0.15",
  "permissions": ["history", "bookmarks", "storage", "favicon"],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "assets/icon16.png",
    "32": "assets/icon32.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  }
}
