{
  "manifest_version": 3,
  "name": "Eternal History",
  "version": "1.0",
  "permissions": ["history", "scripting", "bookmarks"],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "host_permissions": ["<all_urls>"]
}
